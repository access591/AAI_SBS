.page-sidebar-menu > li > ul.sub-menu > li ul.sub-menu {
    text-indent: 3px;
    margin-left: 5px;
    border-left: 1px solid #2b6990;
    background:#2b4a7a;
}

ul.page-sidebar-menu > li > ul.sub-menu li.open > ul.sub-menu li.open > a, ul.page-sidebar-menu > li > ul.sub-menu li.open > ul.sub-menu li.open > a:hover {
    background:#03b9ae !important;
}
.page-sidebar-menu > li > ul.sub-menu > li ul.sub-menu > li ul.sub-menu {
    background:#2b567a;
}

ul.page-sidebar-menu > li > ul.sub-menu li.open > ul.sub-menu li.open > ul.sub-menu li.open > a, ul.page-sidebar-menu > li > ul.sub-menu li.open > ul.sub-menu li.open > ul.sub-menu li.open > a:hover {
    background:#03b990 !important;
}
.page-sidebar-menu > li > ul.sub-menu > li ul.sub-menu > li ul.sub-menu > li ul.sub-menu {
    background:#2b6c7a;
}

/* font color for all sub menu links*/
ul.page-sidebar-menu li > ul.sub-menu > li > a {
  color: #fff; border-bottom:1px solid #2b6990;
}
/* menu arrows */
ul.page-sidebar-menu > li > a .arrow:before,
ul.page-sidebar-menu > li > a .arrow.open:before,
ul.page-sidebar-menu > li > ul.sub-menu a .arrow:before,
ul.page-sidebar-menu > li > ul.sub-menu a .arrow.open:before,
ul.page-sidebar-menu > li > a > .arrow.open:before,
ul.page-sidebar-menu > li.active > a .arrow:before,
ul.page-sidebar-menu > li.active > a .arrow.open:before {
  color: #fff !important;
  opacity: .5;
}
/*ul.page-sidebar-menu > li > ul.sub-menu a .arrow::before
ul.page-sidebar-menu > li > ul.sub-menu a .arrow:before,*/
ul.page-sidebar-menu > li > ul.sub-menu a .arrow:before {
     margin-right:10px;
}

ul.page-sidebar-menu > li > ul.sub-menu > li > ul.sub-menu a .arrow:before, ul.page-sidebar-menu > li > ul.sub-menu > li > ul.sub-menu a .arrow.open:before {
     margin-right: 5px;
}
/***
Sidebar toggler
***/
.sidebar-toggler {
  background-image: url(../Images/sidebar-toggler.jpg);
  background-color: #242424;
}
/* sub menu bg color on hover menu item */
.page-sidebar-closed ul.page-sidebar-menu > li:hover .sub-menu {
  background-color: #3d3d3d;
}
/***
Footer 
***/
.footer .footer-inner {
  color: #C2E2FB;
}
.footer .footer-tools .go-top {
  background-color: #4f4f4f;
}
.footer .footer-tools .go-top:hover {
  opacity: 0.7;
  filter: alpha(opacity=70);
}
.footer .footer-tools .go-top i {
  color: #8a8a8a;
}


/*** Footer Layouts ***/
.page-footer-fixed .footer {
  background-color: #025DA2;
}
.page-footer-fixed .footer .footer-tools .go-top {
    background-color: #02052d;
}
/* end:fixed footer */


@media (max-width: 1199px) {
    .name {
        width: 65%;
    }
   .name-inner {
        width: 57%;
    }
}
@media (max-width: 991px) {
    .name {
        font-size: 18px;
    }
    .name-inner {
        font-size: 18px;
        line-height: inherit;
        width: 100%;
        left: 0;
        margin: 0;
        padding: 5px 0;
        background: #1c3a8f;
        display: block;
    }
    .page-header-fixed .page-container {
        margin-top: 36px !important;
    }
}

@media (max-width: 768px) {
    .name {
        font-size: 17px;
    }
 
}

@media (max-width: 680px) {
    .name-inner {
        font-size: 15px;
    }
    .header .navbar-nav > li.dropdown.user .dropdown-toggle .username,
    .header .navbar-nav li.dropdown.user .dropdown-toggle i {
        display: none;
    }
    .header .navbar-nav > li.dropdown.user .dropdown-toggle {
        padding: 6px;
    }
    .page-header-fixed .page-container {
        margin-top: 30px !important;
    }
    .header .navbar-nav > li.dropdown .dropdown-toggle {
        padding: 13px 5px 8px;
    }
    .header .navbar-nav > li.dropdown {
        padding-left: 0;
    }
}

@media (max-width: 380px) {
    .name, .name-inner {
        font-size: 12px !important;
        line-height: 20px !important;
    }
}
